@{
}
@model List<Bloggie.Web.Models.Domain.Tag>
<h1>Tags</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Display Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var tags in Model)
        {
            <tr>
                <td>@tags.Name</td>
                <td>@tags.DisplayName</td>
            </tr>
        }
    </tbody>
</table>