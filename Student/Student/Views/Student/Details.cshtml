@{
}
@model Student.Models.Entities.Student1;
<h1>Student Details</h1>
<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Email</td>
            <td>Phone</td>
            <td>Address</td>
            <td>Date</td>
            <td>Status</td>
            <td> </td>
        </tr>
    </thead>
    <tbody>
            <tr>
            <td>@Model.Id</td>
            <td>@Model.Name</td>
            <td>@Model.Email</td>
            <td>@Model.Phone</td>
            <td>@Model.Address</td>
            <td>@Model.Date</td>
            <td>@Model.Status</td>
            <td>
                <a class="btn btn-primary" asp-action="Edit" asp-controller="Student" asp-route-id="@Model.Id">Edit</a>
            </td>
            </tr>
    </tbody>
</table>