@{
}
@model List<Student.Models.Entities.Student1>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Email</td>
            <td>Phone</td>
            <td>Address</td>
            <td>Date</td>
            <td>Status</td>
            <td>   </td>
            <td>   </td>
        </tr>
    </thead>
    <tbody>
        @foreach(var s in Model)
        {
            <tr>
                <td>@s.Id</td>
                <td>@s.Name</td>
                <td>@s.Email</td>
                <td>@s.Phone</td>
                <td>@s.Address</td>
                <td>@s.Date</td>
                <td>@s.Status</td>
                <td>
                    <a class="btn btn-primary" asp-action="Edit" asp-controller="Admin" asp-route-id="@s.Id">Edit</a>
                </td>
                <td>
                    <a class="btn btn-danger" asp-action="Delete" asp-controller="Admin" asp-route-id="@s.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<div class="mt-3">
    <a class="btn btn-primary" asp-action="Index" asp-controller="Admin">Home</a>
</div>